{
    "name": "amir/model-key-factory",
    "description": "A simple package to generate Eloquent Model Keys",
    "keywords": [
        "laravel",
        "model-key-factory"
    ],
    "license": "MIT",
    "authors": [
        {
            "role": "Developer",
            "name": "Amir Kacem Ben Ammou",
            "email": "amir.kassem.ben.ammou@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Eloquent\\KeyFactory\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {

            "Eloquent\\KeyFactory\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eloquent\\KeyFactory\\KeyFactoryServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.3",
        "nunomaduro/collision": "^6.1"
    }

}
